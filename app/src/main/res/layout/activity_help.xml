<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HelpActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="414dp"
        android:layout_height="60dp"
        android:background="@color/moreopen_blue2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/profileimage"
            android:layout_width="56dp"
            android:layout_height="51dp"
            android:layout_marginStart="4dp"
            android:layout_marginBottom="4dp"
            android:src="@drawable/twotone_account_circle_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="94dp"
            android:layout_height="34dp"
            android:layout_marginEnd="188dp"
            android:layout_marginBottom="12dp"
            android:text="Help"
            android:textColor="@color/white"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.121"
            app:layout_constraintStart_toEndOf="@+id/profileimage" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout10"
        android:layout_width="410dp"
        android:layout_height="56dp"
        android:layout_marginTop="268dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/telegram"
            android:layout_width="101dp"
            android:layout_height="29dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="-8dp"
            android:layout_marginBottom="11dp"
            android:layout_toStartOf="@+id/username"
            android:layout_toEndOf="@+id/telegramlogo"
            android:text="Telegram:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="10dp"
            android:text="haykpandunts"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/telegramlogo"
            android:layout_width="52dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="0dp"
            android:src="@drawable/tg"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="410dp"
        android:layout_height="58dp"
        android:layout_marginTop="340dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp">

        <TextView
            android:id="@+id/email"
            android:layout_width="96dp"
            android:layout_height="21dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="-94dp"
            android:layout_marginBottom="29dp"
            android:layout_toStartOf="@+id/emailname"
            android:layout_toEndOf="@+id/emaillogo"
            android:text="Email:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/emailname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="2dp"
            android:text="panduntshayk2008@gmail.com"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/emaillogo"
            android:layout_width="52dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="0dp"
            android:src="@drawable/gmailemail"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="412dp"
        android:layout_height="63dp"
        android:layout_marginTop="424dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-1dp">

        <TextView
            android:id="@+id/telephone"
            android:layout_width="192dp"
            android:layout_height="26dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="19dp"
            android:layout_marginEnd="-191dp"
            android:layout_marginBottom="27dp"
            android:layout_toStartOf="@+id/number"
            android:layout_toEndOf="@+id/telephonelogo"
            android:text="Telephone Number:"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="183dp"
            android:layout_marginBottom="1dp"
            android:text="+37494197021"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/telephonelogo"
            android:layout_width="52dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="18dp"
            android:layout_marginTop="0dp"
            android:src="@drawable/bluephoneicon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>