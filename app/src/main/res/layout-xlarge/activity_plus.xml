<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="525dp"
    android:layout_height="1050dp"
    android:gravity="center"
    android:orientation="vertical"
    android:background="@drawable/background"
    tools:context=".PlusActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout3"
        android:layout_width="514dp"
        android:layout_height="78dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/round_back_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/editTextEnergyPrice1"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="8dp"
            android:text="    Էլ․Էներգիայաի սակագին"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/editTextEnergyPrice"
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="47dp"
            android:layout_marginEnd="117dp"
            android:text="    Էլ․Էներգիայաի սակագին"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="518dp"
        android:layout_height="79dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/round_back_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout3">

        <TextView
            android:id="@+id/editTextPower1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="11dp"
            android:text="    Արևային Կայանի հզորություն/ԿՎՏ"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/editTextPower"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="41dp"
            android:text="    Արևային Կայանի հզորություն/ԿՎՏ"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="518dp"
        android:layout_height="66dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/round_back_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout2">

        <TextView
            android:id="@+id/editTextProductivity1"
            android:layout_width="391dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-3dp"
            android:layout_marginTop="4dp"
            android:text="    Արտադրողականություն"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/editTextProductivity"
            android:layout_width="391dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="35dp"
            android:text="    Արտադրողականություն"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="518dp"
        android:layout_height="61dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/round_back_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout">

        <TextView
            android:id="@+id/editTextUnitCost1"
            android:layout_width="391dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="7dp"
            android:text="    Կայանի Միավոր ԿՎՏ-ի արժեքը"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/editTextUnitCost"
            android:layout_width="391dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="1dp"
            android:layout_marginTop="32dp"
            android:text="    Կայանի Միավոր ԿՎՏ-ի արժեքը"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="514dp"
        android:layout_height="117dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/round_back_white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout4">

        <TextView
            android:id="@+id/editTextMaintenanceCost1"
            android:layout_width="398dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="    20 տարվա կայանի շահագործման ծախսեր"
            android:textColor="@color/white"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/editTextMaintenanceCost"
            android:layout_width="402dp"
            android:layout_height="46dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="6dp"
            android:layout_marginTop="59dp"
            android:text="    20 տարվա կայանի շահագործման ծախսեր"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
